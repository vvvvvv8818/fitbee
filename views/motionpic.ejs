<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="video_style.css">
    <%- mycss2.style %>
</head>
<script language="JavaScript">
    var SetTime = 5;		// 최초 설정 시간(기본 : 초)
    function msg_time() {	// 1초씩 카운트
        m = Math.floor(SetTime % 60);	// 남은 시간 계산
        var msg = "<font color='red'>" + m + "</font>";
        var span = document.getElementById("span");
        span.style.fontSize = "250px";
        span.innerHTML = msg;		// div 영역에 보여줌 
        SetTime--;					// 1초씩 감소
        if (SetTime < 0) {			// 시간 종료
            clearInterval(tid);
           // setTimeout(function () { location.href = "Upper1.html"; }, 5000);
        }
    }
    window.onload = function TimerStart() { tid = setInterval('msg_time()', 1000) };
</script>

<body>
    <div class="top">
        <h1>Follow Motion After 5 sec.</h1>
    </div>
    <div class='motionpose'>
        <p style="text-align: center;">
        <img src="C:/Users/qkrjh/Desktop/nodjs/FItbee/images/pic1.png">
    </p>
    </div>
    <div class="count">
        <div id="span"></div>
    </div>
</body>

</html>